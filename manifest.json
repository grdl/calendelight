{
    "manifest_version": 2,

    "name": "Calendelight",
    "description": "This extension adds a Google Calendar button to scheduled Delight Yoga classes.",
    "version": "1.0",
    
    "content_scripts": [
      {
        "matches": ["https://delightyoga.com/my-delight"],
        "js": ["moment.min.js", "calendelight.js"]          
      }
    ],

    "permissions": [
        "activeTab"
    ]
}